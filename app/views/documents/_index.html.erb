<% @documents.each do |document| %>
    <tr>
    <td><%= document.support %></td>
    <td><%= document.nature %></td>
    <td><%= document.name %></td>
    <td>
	    <div class="action-buttons">
			<%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_document_path(document), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), document_delete_path(document), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
    </tr>
<% end %>